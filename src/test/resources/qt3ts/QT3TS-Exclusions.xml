        <xpmt:ignore test="RangeExpr-409d"/>
    </xpmt:dependency>
    <xpmt:dependency>
        <xpmt:reason code="BBO-CPP">Issue with cp protocol: see https://github.com/cmarchand/cp-protocol/issues/1</xpmt:reason>
