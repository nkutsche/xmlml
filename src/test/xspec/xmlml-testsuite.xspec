            </x:scenario>
        </x:scenario>
        <x:scenario label="escape ' or &quot; in attributes">
            <x:scenario label="double quoted attribute">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml">
                            <element collapsed="true">
                                <name>root</name>
                                <attribute>
                                    <ws space="1"/>
                                    <name>a</name>
                                    <eq/>
                                    <value>
                                        <data>' or "</data>
                                    </value>
                                </attribute>
                            </element>
                        </document>
                    </x:param>
                </x:call>
                <x:expect label="result xml" select="string(.)"><![CDATA[<root a="' or &quot;"/>]]></x:expect>
            </x:scenario>
            <x:scenario label="single quoted attribute">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml">
