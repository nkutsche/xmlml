            </x:scenario>
            <x:scenario label="in a PI (not escaped)">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml">
                            <pi>
                                <name>pi</name>
                                <ws space="1"/>
                                <value>
