            </x:scenario>
        </x:scenario>
        <x:scenario label="insert space if -- occurs in comment">
            <x:call>
                <x:param>
                    <document xmlns="http://www.nkutsche.com/xmlml">
                        <comment>
                            <data>comment with -- </data>
                        </comment>
                        <element collapsed="true">
                            <name>root</name>
                        </element>
                    </document>
                </x:param>
            </x:call>
            <x:expect label="result xml" select="string(.)"><![CDATA[<!--comment with - - --><root/>]]></x:expect>
        </x:scenario>
        <x:scenario label="add space if comments end with -">
            <x:scenario label="just end with '-' -> add space ">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml">
                            <comment>
                                <data>comment end with: -</data>
                            </comment>
                            <element collapsed="true">
                                <name>root</name>
                            </element>
                        </document>
                    </x:param>
                </x:call>
                <x:expect label="result xml" select="string(.)"><![CDATA[<!--comment end with: - --><root/>]]></x:expect>
            </x:scenario>
            <x:scenario label="end with '-\n' -> no space add ">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml" line-feed-format="n">
                            <comment>
                                <data>comment end with: -</data>
                                <nl/>
                            </comment>
                            <element collapsed="true">
                                <name>root</name>
                            </element>
                        </document>
                    </x:param>
                </x:call>
                <x:expect label="result xml" select="string(.)"><![CDATA[<!--comment end with: -]]>&#xA;-->&lt;root/&gt;</x:expect>
            </x:scenario>
            <x:scenario label="end with '-' and empty data -> add space ">
                <x:call>
                    <x:param>
                        <document xmlns="http://www.nkutsche.com/xmlml">
                            <comment>
                                <data>comment end with: -</data>
                                <data/>
                            </comment>
                            <element collapsed="true">
                                <name>root</name>
                            </element>
                        </document>
                    </x:param>
                </x:call>
                <x:expect label="result xml" select="string(.)"><![CDATA[<!--comment end with: - --><root/>]]></x:expect>
            </x:scenario>
        </x:scenario>
        <x:scenario label="insert space if ?> occurs in PI">
            <x:call>
                <x:param>
                    <document xmlns="http://www.nkutsche.com/xmlml">
                        <pi>
                            <name>pi</name>
                            <ws space="1"/>
                            <value>
                                <data>?></data>
                            </value>
                        </pi>
                        <element collapsed="true">
                            <name>root</name>
                        </element>
                    </document>
                </x:param>
            </x:call>
            <x:expect label="result xml" select="string(.)"><![CDATA[<?pi ? >?><root/>]]></x:expect>
        </x:scenario>
        <x:scenario label="insert space if ]]> occurs in CDATA">
            <x:call>
                <x:param>
                    <document xmlns="http://www.nkutsche.com/xmlml">
                        <element>
                            <name>root</name>
                            <content space="preserve">
                                <text>
                                    <cdata-section>
                                        <data>ends of cdata: ]]&gt;</data>
                                    </cdata-section>
                                </text>
                            </content>
                        </element>
                    </document>
                </x:param>
            </x:call>
            <x:expect label="result xml" select="string(.)">&lt;root&gt;&lt;![CDATA[ends of cdata: ]] &gt;]]&gt;&lt;/root&gt;</x:expect>
        </x:scenario>
    </x:scenario>
    
