        <name>from</name>
        <ws space="1"/>
        <inline><nl/>&lt;!ENTITY ent "bar"&gt;<nl/></inline>
    </doc-type-decl>
    <ws nl="1"/>
    <element>
        <name>root</name>
        <attribute>
            <ws space="1"/>
            <name>foo</name>
            <eq/>
            <value><entity name="ent"><text>bar</text></entity></value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>bar</name>
            <eq/>
            <value><entity name="lt"><text>&lt;</text></entity></value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>baz</name>
            <eq/>
            <value><entity codepoint="10"/></value>
