<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" line-feed-format="n" 
    id="xmlml_d54"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/entity/entities-in-attr-mixed.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/entity/entities-in-attr-mixed.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>from</name>
        <ws space="1"/>
        <inline>
            <nl/>
            <data><![CDATA[<!ENTITY ent "bar">]]></data>
            <nl/>
        </inline>
    </doc-type-decl>
    <ws nl="1"/>
    <element>
        <name>root</name>
        <attribute>
            <ws space="1"/>
            <name>foo</name>
            <eq/>
            <value>
                <entity name="ent">
                    <data>bar</data>
                </entity>
            </value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>bar</name>
            <eq/>
            <value>
                <entity name="lt">
                    <data>&lt;</data>
                </entity>
            </value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>baz</name>
            <eq/>
            <value>
                <entity codepoint="10"/>
            </value>
        </attribute>
    </element>
</document>
