    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ENTITY ent "entity-value"><!ELEMENT root EMPTY><!ATTLIST root a CDATA "&ent;">]]></data>
