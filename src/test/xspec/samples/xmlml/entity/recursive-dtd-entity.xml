<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>&lt;!ENTITY foo "foo"&gt;&lt;!ENTITY bar "bar"&gt;&lt;!ENTITY foobar "&amp;foo;-&amp;bar;"&gt;</inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <content>
            <text><entity name="foobar"><text><entity name="foo"><text>foo</text></entity></text><text>-</text><text><entity name="bar"><text>bar</text></entity></text></entity></text>
        </content>
    </element>
</document>
