<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d61"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/entity/recursive-dtd-entity.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/entity/recursive-dtd-entity.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ENTITY foo "foo"><!ENTITY bar "bar"><!ENTITY foobar "&foo;-&bar;">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <content space="preserve">
            <text>
                <entity name="foobar">
                    <entity name="foo">
                        <data>foo</data>
                    </entity>
                    <data>-</data>
                    <entity name="bar">
                        <data>bar</data>
                    </entity>
                </entity>
            </text>
        </content>
    </element>
</document>
