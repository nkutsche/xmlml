<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>&lt;!ENTITY foo "foo"&gt;&lt;!ENTITY bar "bar"&gt;&lt;!ENTITY foobar "&amp;foo;-&amp;bar;"&gt;</inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <content>
            <text><entity type="name">foobar</entity></text>
        </content>
    </element>
</document>
