<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ENTITY foo "foo"><!ENTITY bar "bar"><!ENTITY foobar "&foo;-&bar;">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <content>
            <text>
                <entity name="foobar">
                    <text>
                        <entity name="foo">
                            <text>
                                <data>foo</data>
                            </text>
                        </entity>
                    </text>
                    <text>
                        <data>-</data>
                    </text>
                    <text>
                        <entity name="bar">
                            <text>
                                <data>bar</data>
                            </text>
                        </entity>
                    </text>
                </entity>
            </text>
        </content>
    </element>
</document>
