<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" line-feed-format="n" 
    id="xmlml_d106"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/whitespace/custom-strip-spacing-no-dtd.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/whitespace/custom-strip-spacing-no-dtd.xml">
    <element>
        <name>root</name>
        <content space="skip">
            <ws>
                <nl/>
                <space amount="4"/>
            </ws>
            <element collapsed="true">
                <name>strip</name>
            </element>
            <ws>
                <nl/>
                <space amount="4"/>
            </ws>
            <element>
                <name>strip</name>
                <content space="skip">
                    <ws>
                        <space amount="5"/>
                    </ws>
                </content>
            </element>
            <ws>
                <nl/>
                <space amount="4"/>
            </ws>
            <element>
                <name>preserve</name>
                <content space="preserve">
                    <text>
                        <data>     </data>
                    </text>
                </content>
            </element>
            <ws>
                <nl/>
            </ws>
        </content>
    </element>
</document>