<!--
<!DOCTYPE src [<!ELEMENT src (src-e*)><!ELEMENT src-e (src-c?)><!ELEMENT src-c EMPTY>]>
<src>
    <src-e/>
    <src-e>     </src-e>
</src>
-->

<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" line-feed-format="rn">
    <doc-type-decl>
        <ws space="1"/>
        <name>src</name>
        <ws space="1"/>
        <inline><![CDATA[<!ELEMENT src (src-e*)><!ELEMENT src-e (src-c?)><!ELEMENT src-c EMPTY>]]></inline>
    </doc-type-decl>
    <ws nl="1"/>
    <element>
        <name>src</name>
        <content>
            <ws>
                <nl/>
                <space amount="4"/>
            </ws>
            <element collapsed="true">
                <name>src-e</name>
            </element>
            <ws>
                <nl/>
                <space amount="4"/>
            </ws>
            <element>
                <name>src-e</name>
                <content>
                    <ws space="5"/>
                </content>
            </element>
            <ws nl="1"/>
        </content>
    </element>
</document>
