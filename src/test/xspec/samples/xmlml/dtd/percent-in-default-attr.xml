<!--<!DOCTYPE doc [<!ENTITY % e "foo"><!ELEMENT doc (#PCDATA)><!ATTLIST doc a1 CDATA "%e;">]><doc></doc>-->
<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d37"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/dtd/percent-in-default-attr.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/dtd/percent-in-default-attr.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>doc</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ENTITY % e "foo"><!ELEMENT doc (#PCDATA)><!ATTLIST doc a1 CDATA "%e;">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>doc</name>
        <attribute default="true" type="CDATA">
            <ws space="1"/>
            <name>a1</name>
            <eq/>
            <value>
                <data>%e;</data>
            </value>
        </attribute>
    </element>
</document>
