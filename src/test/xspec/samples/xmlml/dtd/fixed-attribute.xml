<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d34"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/dtd/fixed-attribute.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/dtd/fixed-attribute.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ELEMENT root EMPTY><!ATTLIST root attribute CDATA #FIXED "fix-value">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <attribute default="true" namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <attribute default="true" type="CDATA">
            <ws space="1"/>
            <name>attribute</name>
            <eq/>
            <value>
                <data>fix-value</data>
            </value>
        </attribute>
    </element>
</document>
