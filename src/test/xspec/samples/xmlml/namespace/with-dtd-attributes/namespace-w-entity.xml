<!--
<!DOCTYPE root [<!ENTITY ns "my.namespace.com"><!ENTITY defns "default.namespace.com">]><ns:root xmlns="&defns;" xmlns:ns="&ns;" xmlns:ns2="&#x6D;y.namespace.com"><el ns2:a="v"/></ns:root>
-->
<?xml-model href="../../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d92"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/namespace/with-dtd-attributes/namespace-w-entity.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/namespace/with-dtd-attributes/namespace-w-entity.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ENTITY ns "my.namespace.com"><!ENTITY defns "default.namespace.com">]]></data>
        </inline>
    </doc-type-decl>
    <element element-default-namespace="default.namespace.com" xmlns:ns="my.namespace.com" xmlns:ns2="my.namespace.com">
        <name>ns:root</name>
        <attribute default="true" namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <attribute namespace="true">
            <ws space="1"/>
            <name/>
            <eq/>
            <value>
                <entity name="defns">
                    <data>default.namespace.com</data>
                </entity>
            </value>
        </attribute>
        <attribute namespace="true">
            <ws space="1"/>
            <name>ns</name>
            <eq/>
            <value>
                <entity name="ns">
                    <data>my.namespace.com</data>
                </entity>
            </value>
        </attribute>
        <attribute namespace="true">
            <ws space="1"/>
            <name>ns2</name>
            <eq/>
            <value>
                <entity codepoint="x6D"/>
                <data>y.namespace.com</data>
            </value>
        </attribute>
        <content space="preserve">
            <element collapsed="true">
                <name>el</name>
                <attribute nsref="xml"/>
                <attribute nsref="#"/>
                <attribute nsref="ns"/>
                <attribute nsref="ns2"/>
                <attribute>
                    <ws space="1"/>
                    <name>ns2:a</name>
                    <eq/>
                    <value>
                        <data>v</data>
                    </value>
                </attribute>
            </element>
        </content>
    </element>
</document>