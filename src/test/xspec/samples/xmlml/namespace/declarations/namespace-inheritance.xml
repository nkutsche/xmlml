<?xml-model href="../../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d82"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/namespace/declarations/namespace-inheritance.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/namespace/declarations/namespace-inheritance.xml">
    <element xmlns:ns="namespace.com">
        <name>ns:root</name>
        <attribute default="true" namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <attribute namespace="true">
            <ws space="1"/>
            <name>ns</name>
            <eq/>
            <value>
                <data>namespace.com</data>
            </value>
        </attribute>
        <content space="preserve">
            <element collapsed="true">
                <name>ns:el</name>
                <attribute nsref="xml"/>
                <attribute nsref="ns"/>
            </element>
        </content>
    </element>
</document>
