<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml" 
    id="xmlml_d1"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/attribute/combine-all.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/attribute/combine-all.xml">
    <element>
        <name>root</name>
        <attribute default="true"
            namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <attribute>
            <ws space="6"/>
            <name>attribute</name>
            <ws space="3"/>
            <eq/>
            <ws space="3"/>
            <value>
                <data>value</data>
            </value>
        </attribute>
        <attribute>
            <ws space="6"/>
            <name>attribute2</name>
            <ws space="3"/>
            <eq/>
            <ws space="3"/>
            <value quotes="single">
                <data>value2</data>
            </value>
        </attribute>
        <ws space="3"/>
    </element>
</document>
