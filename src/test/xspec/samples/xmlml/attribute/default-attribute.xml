<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ELEMENT root EMPTY><!ATTLIST root attribute CDATA "default">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <attribute default="true" type="CDATA">
            <ws space="1"/>
            <name>attribute</name>
            <eq/>
            <value>
                <data>default</data>
            </value>
        </attribute>
    </element>
</document>
