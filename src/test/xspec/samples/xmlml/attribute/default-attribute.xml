<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>&lt;!ELEMENT root EMPTY&gt;&lt;!ATTLIST root attribute CDATA "default"&gt;</inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <attribute default="true">
            <ws space="1"/>
            <name>attribute</name>
            <eq/>
            <value>default</value>
        </attribute>
    </element>
</document>