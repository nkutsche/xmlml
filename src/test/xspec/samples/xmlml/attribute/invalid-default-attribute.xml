<!--
<!DOCTYPE root [<!ELEMENT root EMPTY><!ATTLIST unknown attribute CDATA "default">]><root><unknown/></root>
-->

<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml"
    id="xmlml_d4_1"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/attribute/invalid-default-attribute.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/attribute/invalid-default-attribute.xml">
    <doc-type-decl>
        <ws space="1"/>
        <name>root</name>
        <ws space="1"/>
        <inline>
            <data><![CDATA[<!ELEMENT root EMPTY><!ATTLIST unknown attribute CDATA "default">]]></data>
        </inline>
    </doc-type-decl>
    <element>
        <name>root</name>
        <attribute default="true"
            namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <content space="preserve">
            <element collapsed="true">
                <name>unknown</name>
                <attribute nsref="xml"/>
                <attribute default="true" type="CDATA">
                    <ws space="1"/>
                    <name>attribute</name>
                    <eq/>
                    <value>
                        <data>default</data>
                    </value>
                </attribute>
            </element>
        </content>
    </element>
</document>