<?xml-model href="../../../../../main/resources/rnc/xmlml.rnc" type="application/relax-ng-compact-syntax"?>
<document xmlns="http://www.nkutsche.com/xmlml"
    id="xmlml_d11"
    document-uri="http://www.nkutsche.com/xmlml/sample-base/attribute/tab-as-value.xml"
    xml:base="http://www.nkutsche.com/xmlml/sample-base/attribute/tab-as-value.xml">
    <element>
        <name>root</name>
        <attribute default="true" namespace="true">
            <ws space="1" />
            <name>xml</name>
            <eq />
            <value>
                <data>http://www.w3.org/XML/1998/namespace</data>
            </value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>tab-char</name>
            <eq/>
            <value>
                <data>foo		bar</data>
            </value>
        </attribute>
        <attribute>
            <ws space="1"/>
            <name>tab-entity</name>
            <eq/>
            <value>
                <data>foo</data>
                <entity codepoint="x9"/>
                <entity codepoint="x9"/>
                <data>bar</data>
            </value>
        </attribute>
    </element>
</document>
